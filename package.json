{
  "name": "barrel-me",
  "version": "0.0.0",
  "description": "Creates a index.js / index.ts file with exported files from a folder",
  "bin": "src/index.js",
  "files": ["src"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonyw4/barrel-me.git"
  },
  "keywords": [
    "create-index-js",
    "create",
    "index",
    "index",
    "barrel",
    "barrel",
    "file",
    "js",
    "index",
    "ts",
    "index"
  ],
  "author": "Jonathan CÃ©lio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonyw4/barrel-me/issues"
  },
  "homepage": "https://github.com/jonyw4/barrel-me#readme",
  "dependencies": {
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "codecov": "3.7.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "semantic-release": "17.0.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  }
}
